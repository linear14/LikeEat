<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >
    <item>
        <shape
            android:shape="rectangle">
            <solid android:color="@color/black01"/>
            <padding
                android:bottom="@dimen/padding_normal"
                android:left="@dimen/padding_normal"
                android:right="@dimen/padding_normal"
                android:top="@dimen/padding_normal"/>
            <corners
                android:bottomLeftRadius="@dimen/corner_radius_normal"
                android:bottomRightRadius="@dimen/corner_radius_normal"
                android:topLeftRadius="@dimen/corner_radius_normal"
                android:topRightRadius="@dimen/corner_radius_normal"/>
        </shape>
    </item>
</layer-list>
